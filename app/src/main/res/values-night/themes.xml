<resources xmlns:tools="http://schemas.android.com/tools">
    <!-- Base application theme. -->
    <style name="Base.Theme.CIRCLEBAR" parent="Theme.Material3.DayNight.NoActionBar">
        <!-- Customize your dark theme here. -->
        <!-- <item name="colorPrimary">@color/my_dark_primary</item> -->
        <item name="colorPrimary">@color/black</item>
        <item name="android:windowBackground">@color/black</item>
        <item name="cardBackgroundColor">@color/grey</item>


    </style>

    <style name="Theme.CIRCLEBAR" parent="Base.Theme.CIRCLEBAR" />

    <style name="Theme.CIRCLEBAR.NoActionBar">
        <item name="windowActionBar">false</item>
        <item name="windowNoTitle">true</item>
        <item name="android:statusBarColor">@android:color/transparent</item>
    </style>

    <style name="Theme.CIRCLEBAR.AppBarOverlay" parent="ThemeOverlay.AppCompat.Dark.ActionBar" />

    <style name="Theme.CIRCLEBAR.PopupOverlay" parent="ThemeOverlay.AppCompat.Light" />
</resources>